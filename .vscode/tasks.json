{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build-windows",
      "command": "dotnet",
      "type": "process",
      "args": [
        "build",
        "${workspaceFolder}/xptv/xptv.csproj",
        "/p:TargetFramework=net9.0-windows10.0.19041.0",
        "/p:Configuration=Debug",
        "/p:GenerateFullPaths=true",
        "/consoleloggerparameters:NoSummary"
      ],
      "problemMatcher": "$msCompile",
      "group": {
        "kind": "build",
        "isDefault": true
      }
    }
  ]
}
